import java.util.*;

 // Compiler version JDK 11.0.2

 class main
 {
  static int convert (int x)
  {
    if (x==0 || x==1)
  { 
    return x;
  }
   int  i=1, result=1;
   while (result<=x)
   {
     i++;
     result=i*i;
   } return i-1;
   }
  
   public static void main(String args[])
   { 
    Scanner input= new Scanner (System.in);
    System.out.print("Enter a number: ");
    int x=input.nextInt();
    System.out.print(convert (x));
   }
 }
